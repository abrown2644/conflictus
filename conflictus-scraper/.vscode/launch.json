    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Conflictus-ScraperV2",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/app.ts",
            "runtimeExecutable": "yarn",
            "runtimeArgs": ["run", "debug"],
            "outFiles": ["${workspaceFolder}/**/*.js"],
            "console": "integratedTerminal",
            "sourceMaps": true,
            "cwd": "${workspaceFolder}"
        }
      ]
